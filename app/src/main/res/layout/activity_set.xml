<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:example="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_set"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.apple.jqjz.activity.SetActivity">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/bgblue">
        <TextView
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="设置中心"
            android:textColor="@color/white"
            android:textSize="21sp"
            android:textStyle="bold"/>
    </RelativeLayout>
    <com.example.apple.jqjz.view.SetItemView
        android:id="@+id/siv_set_br"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        example:siBackground="first"
        example:siTitle="开启后台运行"
        example:siToggle="true"
        >
    </com.example.apple.jqjz.view.SetItemView>
    <com.example.apple.jqjz.view.SetItemView
        android:id="@+id/siv_set_receive"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        example:siBackground="last"
        example:siTitle="接受报警信息"
        example:siToggle="true"
        >
    </com.example.apple.jqjz.view.SetItemView>
    <!--<Button
        android:id="@+id/bt_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="开启服务"
        android:clickable="true"/>
    <Button
        android:id="@+id/bt_stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭服务"
        android:clickable="true"/>-->

</LinearLayout>
